{
  "name": "@lotsof/i18n",
  "version": "1.0.0",
  "description": "Simple but powerfull package that let you handle i18n of your websites/apps easily and elegantly",
  "type": "module",
  "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
  "license": "MIT",
  "scripts": {
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@coffeekraken/s-duration": "^2.0.0-alpha.27",
    "@coffeekraken/s-env": "^2.0.0-alpha.27",
    "@coffeekraken/s-glob": "^2.0.0-alpha.27",
    "@lotsof/sugar": "^1.0.0",
    "@ts-ast-parser/core": "^0.2.0",
    "@ts-ast-parser/readers": "^0.2.0",
    "ntypescript": "^1.201706190042.1",
    "typescript": "^5.4.5"
  },
  "main": "dist/pkg/cjs/shared/exports.js",
  "module": "dist/pkg/esm/shared/exports.js",
  "exports": {
    ".": {
      "require": "./dist/pkg/cjs/shared/exports.js",
      "import": "./dist/pkg/esm/shared/exports.js"
    },
    "./shared/*": {
      "require": "./dist/pkg/cjs/shared/*.js",
      "import": "./dist/pkg/esm/shared/*.js"
    },
    "./node/*": {
      "require": "./dist/pkg/cjs/node/*.js",
      "import": "./dist/pkg/esm/node/*.js"
    },
    "./js/*": {
      "require": "./dist/pkg/cjs/js/*.js",
      "import": "./dist/pkg/esm/js/*.js"
    }
  },
  "homepage": "https://lotsof.dev"
}
